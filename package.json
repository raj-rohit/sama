{
  "name": "components-hero",
  "version": "0.1.0",
  "main": "dist/hero.umd.js",
  "module": "dist/hero.esm.js",
  "unpkg": "dist/hero.min.js",
  "browser": {
    "./sfc": "src/Hero.vue"
  },
  "files": [
    "dist/*",
    "src/*",
    "attributes.json",
    "tags.json"
  ],
  "vetur": {
    "tags": "tags.json",
    "attributes": "attributes.json"
  },
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/Hero.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/Hero.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/Hero.min.js"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "minimist": "^1.2.0",
    "rollup": "^1.14.4",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-uglify-es": "0.0.1",
    "rollup-plugin-vue": "^4.7.2",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  }, "repository": {
    "type": "git",
    "url": "git+https://github.com/raj-rohit/sama.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@samp"
  }
}
